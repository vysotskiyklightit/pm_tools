[flake8]
max-line-length = 79
exclude =
    .git,
    __pycache__,
    .env,
    venv/*,
    **/core
    **/settings
ignore =
    H306,  # Imports not in alphabetical order
    D101,  # Missing docstring in public class
    W503,  # Line break before binary operator - deprecated
    F405,
    F403,
    F401,
